<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.layout.*?>
<?import javafx.scene.text.Text?>

<?import javafx.scene.image.ImageView?>
<?import com.example.movieapp.components.MovieRating?>
<?import javafx.scene.control.Button?>

<fx:root stylesheets="@../styles.css" type="javafx.scene.layout.VBox" xmlns:fx="http://javafx.com/fxml" alignment="CENTER" prefWidth="33">
    <ImageView styleClass="moviePosterImage" fx:id="moviePoster" />
    <Text fill="#777777" styleClass="moviesTitle" fx:id="movieTitle"/>
    <MovieRating styleClass="moviesRating" fx:id="movieRating" />
    <Button styleClass="detailMovieButton" fx:id="movieDetailNavBtn" />
</fx:root>


